{
  "name": "TodoApp",
  "version": "0.1.0",
  "private": true,
  "main": "server/server.js",
  "scripts": {
    "build:js": "webpack",
    "lint:js": "(node node_modules/eslint/bin/eslint.js --config .eslintrc --format checkstyle --output-file build/reports/checkstyle/eslint.xml src/** && echo ESLint finished without errors!)|| echo ESLint failed!",
    "postinstall": "npm run build:js",
    "start": "node server/server.js",
    "test": "cross-env NODE_ENV=test karma start",
    "test:watch": "cross-env NODE_ENV=test karma start --autoWatch=true --singleRun=false",
    "dev": "concurrently --kill-others \"npm run watch:js\" \"npm run watch:server\"",
    "watch:js": "webpack-dev-server --port=8081 --content-base public/ --progress --colors",
    "watch:server": "nodemon --watch server/server.js"
  },
  "dependencies": {
    "body-parser": "^1.4.3",
    "es6-promise": "^3.2.1",
    "express": "^4.4.5",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "whatwg-fetch": "^1.0.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.4.0",
    "babel-core": "^6.10.4",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-istanbul": "^2.0.0",
    "babel-preset-airbnb": "^2.0.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^3.5.0",
    "concurrently": "^2.1.0",
    "cross-env": "^2.0.0",
    "css-loader": "^0.24.0",
    "enzyme": "^2.3.0",
    "eslint": "^3.1.1",
    "eslint-plugin-react": "^5.2.2",
    "exports-loader": "^0.6.3",
    "extract-text-webpack-plugin": "^1.0.1",
    "imports-loader": "^0.6.5",
    "json-loader": "^0.5.4",
    "karma": "^0.13.22",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^1.0.1",
    "karma-coverage": "^1.1.1",
    "karma-junit-reporter": "^1.1.0",
    "karma-mocha": "^1.0.1",
    "karma-phantomjs-launcher": "^1.0.1",
    "karma-sinon": "^1.0.5",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.5.3",
    "nodemon": "^1.9.1",
    "postcss-loader": "^0.10.1",
    "precss": "^1.4.0",
    "react-addons-test-utils": "^15.2.1",
    "sinon": "^1.17.4",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  }
}
